    "dev-server": "webpack-dev-server src/index.js --content-base contrib/ui",
    "prepublish": "node contrib/build.js",
    "style": "jscs src"
  },
  "bin": {
    "esper": "./contrib/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/codecombat/esper.js.git"
  },
  "keywords": [
    "esper",
    "javascript",
    "interperter",
    "ast",
    "eval"
